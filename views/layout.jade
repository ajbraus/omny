doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')

    block vars
    
    title #{titleText}
    meta(name='description', content='#{descriptionText}')

    meta(name='author', content='Omny Team')
    meta(name="keywords" content="Teleportation, Teleport, Telepresence, Rent a Double, Robot Rental")

    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')

    title Omny - Teleportation Services for Hire
      
    meta(property='og:title', content='Teleport to San Francisco for $100')
    meta(property='og:description', content='Share your code to be first in line for the alpha release.')
    meta(property='og:type', content='website')
    meta(property='og:url', content='http://www.beomny.com')
    meta(property='og:image', content='https://upload.wikimedia.org/wikipedia/commons/e/ec/SCP_Foundation_(emblem).svg')

    // Bootstrap Core CSS
    link(href='/vendor/bootstrap/dist/css/bootstrap.min.css', rel='stylesheet')
    
    //- ANIMATE.CSS
    link(rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/animate.css/3.2.0/animate.min.css")
      
    //- IONICONS
    link(href="http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css", rel='stylesheet')

    //- Custom CSS
    link(href='/stylesheets/style.css', rel='stylesheet')

    //- Custom Fonts
    link(href="/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css")

    // HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js')

  body
    block content

    section.footer
      .container
        .row
          .col-md-4
            p 
              a(href="/") Home
            p 
              a(href="/bookings/new") Make a Reservation
            p 
              a(href="/about-us") About Us
            p 
              a(href="/faq") FAQ
            p 
              a(href="/blog") Blog
            p 
              a(href="/contact-us") Contact
          .col-md-4
          .col-md-4
            p 
              .copyright Copyright Â© Omny LLC 2016

    
    //- SCRIPTS

    script(type="text/javascript" src='/vendor/jquery/dist/jquery.min.js')
    script(type="text/javascript" src='/vendor/bootstrap/dist/js/bootstrap.min.js')

    script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyDKk2JgZEwGLEsq11G-NLv8j_vMMT-suzw&libraries=places&callback=initAutocomplete', async='', defer='')

    script(type="text/javascript" src='/vendor/moment/min/moment.min.js')

    //- MY SCRIPTS
    script(type="text/javascript" src='/javascripts/contacts.js')
    script(type="text/javascript" src='/javascripts/bookings.js')
